{
	"name": "nodejs-environment-development-in-docker",
	"version": "1.7.8",
	"private": true,
	"description": "",
	"engines": {
		"node": ">=18.17.1"
	},
	"scripts": {
		"start": "npm run build && node dist/index.js",
		"build": "rimraf ./dist && tsc",
		"dev": "ts-node-dev --inspect --respawn src/index.ts",
		"devnodemon": "nodemon --signal SIGINT --inspect=0.0.0.0:5850 --nolazy src/index.ts",
		"container": "docker-compose up --build --force-recreate",
		"container:dev": "npm prune && npm install && npm run dev",
		"test": "node test.js"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@types/express": "^4.17.17",
		"@types/json-schema": "^7.0.13",
		"@types/pino-http": "^5.8.1",
		"@typescript-eslint/eslint-plugin": "^6.7.2",
		"@typescript-eslint/parser": "^6.7.2",
		"eslint": "^8.49.0",
		"eslint-config-standard-with-typescript": "^39.0.0",
		"eslint-import-resolver-typescript": "^3.6.0",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-n": "^16.1.0",
		"eslint-plugin-promise": "^6.1.1",
		"nodemon": "^2.0.22",
		"pino": "^8.15.1",
		"pino-http": "^8.5.0",
		"rimraf": "^5.0.1",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.2.2"
	},
	"dependencies": {
		"axios": "^1.5.0",
		"express": "^4.18.2",
		"uuid": "^7.0.2"
	}
}
